    "build": "yarn build:compile"
  },
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/CodificationOrg/cutwater.git",
    "directory": "packages/cutwater-build-typescript"
  },
  "keywords": [
    "common",
    "cutwater",
    "utilities",
    "support",
    "build",
    "gulp",
    "task",
    "codification",
    "typescript",
    "lint",
    "tsc"
  ],
  "author": {
    "name": "William Weiss",
    "url": "https://github.com/wweiss"
  },
  "license": "Apache-2.0",
  "private": false,
  "eslintConfig": {
    "extends": [
      "@codification/cutwater"
    ]
  },
  "prettier": "@codification/cutwater-build-core/includes/prettier-config",
  "dependencies": {
    "@codification/cutwater-build-core": "^0.34.6",
    "@codification/eslint-config-cutwater": "^0.34.6",
    "eslint": "8.6.0",
    "gulp-eslint": "^6.0.0",
    "typescript": "^4.1.3"
  }
}
