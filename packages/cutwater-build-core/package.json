  },
  "license": "Apache-2.0",
  "private": false,
  "prettier": "./includes/prettier-config",
  "dependencies": {
    "@types/gulp": "^4.0.6",
    "@types/jest": "^25.x",
    "@types/node": "^12.7.4",
    "ajv": "^6.10.2",
    "colors": "^1.3.3",
    "cross-spawn": "^7.0.1",
    "del": "^5.1.0",
    "end-of-stream": "^1.4.1",
