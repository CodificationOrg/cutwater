    "gulp-prettier": "^2.2.0",
    "jest": "^25.x",
    "node-notifier": "^6.0.0",
    "prettier": "^1.18.2",
    "pretty-hrtime": "^1.0.3",
    "spawn-args": "^0.2.0",
    "through2": "^3.0.1",
