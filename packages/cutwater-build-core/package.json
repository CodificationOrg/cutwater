  },
  "license": "Apache-2.0",
  "private": false,
  "dependencies": {
    "@microsoft/gulp-core-build": "^3.9.23",
    "typedoc": "^0.14.2",
    "typedoc-plugin-markdown": "^1.2.0"
  },
  "devDependencies": {
    "@microsoft/rush-stack-compiler-3.2": "^0.2.19"
  },
  "jest": {
    "testURL": "http://localhost"
