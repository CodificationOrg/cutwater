{
  "name": "cutwater-nx-plugin",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/cutwater-nx-plugin/src",
  "projectType": "library",
  "targets": {
    "build": {
      "executor": "@nx/js:swc",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/cutwater-nx-plugin",
        "main": "packages/cutwater-nx-plugin/src/index.ts",
        "tsConfig": "packages/cutwater-nx-plugin/tsconfig.lib.json",
        "assets": [
          "packages/cutwater-nx-plugin/*.md",
          {
            "input": "./packages/cutwater-nx-plugin/src",
            "glob": "**/!(*.ts)",
            "output": "./src"
          },
          {
            "input": "./packages/cutwater-nx-plugin/src",
            "glob": "**/*.d.ts",
            "output": "./src"
          },
          {
            "input": "./packages/cutwater-nx-plugin",
            "glob": "generators.json",
            "output": "."
          },
          {
            "input": "./packages/cutwater-nx-plugin",
            "glob": "executors.json",
            "output": "."
          }
        ]
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": [
          "packages/cutwater-nx-plugin/**/*.ts",
          "packages/cutwater-nx-plugin/package.json",
          "packages/cutwater-nx-plugin/executors.json"
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/cutwater-nx-plugin/jest.config.ts",
        "passWithNoTests": true
      },
      "configurations": {
        "ci": {
          "ci": true,
          "codeCoverage": true
        }
      }
    },
    "deploy": {
      "executor": "ngx-deploy-npm:deploy"
    }
  },
  "tags": []
}
